# Azure AD Application Configuration
AZURE_TENANT_ID=your_tenant_id_here
AZURE_CLIENT_ID=your_client_id_here
AZURE_CLIENT_SECRET=your_client_secret_here

# Microsoft 365 User Configuration
# Set this to the user's email or ID for whom you want to manage contacts
# Use 'me' to refer to the currently authenticated user
USER_ID=me

# Optional: Logging level (debug, info, warn, error)
LOG_LEVEL=info

# Optional: Server port (if running as a web server)
# PORT=3000

# Optional: Enable/disable request/response logging
# REQUEST_LOGGING=true

# Optional: Enable/disable detailed error messages in responses
# NODE_ENV=development
